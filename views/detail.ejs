<% // Render individual movie details here %>
<% // This is how to do comments in EJS, by the way... %>
<h2>Movie details for : </h2>
<h1> <%=results.Title %> </h1>
<img src="<%= results.Poster%>"/>

<% for(const index in results){ %>
<div>
    <h3><%= index%>: </h3>
    <p><%= results[index]%></p>
<% if(index === 'Ratings'){ %>
    <% for(const inside in results[index]) { %>
        <h3><%= inside%></h3>
        <p><%= results[index][inside]%></p>
    <% } %>
 <%}%>

</div>
    
<%} %>
